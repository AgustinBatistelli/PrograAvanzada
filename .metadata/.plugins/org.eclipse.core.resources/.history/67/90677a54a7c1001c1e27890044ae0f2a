package lab;

import java.util.*; // preguntar xq necesito volve a llamar a la biblioteca.

public abstract class Pack extends Producto {
	protected LinkedList<Producto> listaProductos = new LinkedList<>();

	public Pack(double precio, String nombreArticulo) {
		super(precio, nombreArticulo); // tiene que llamar a super.
	}
		
	abstract protected double calcularPrecio();
	abstract protected void agregarProducto(Producto prod);
	abstract protected void agregarProducto(Producto prod, boolean esGratis);
	
	
	
}
