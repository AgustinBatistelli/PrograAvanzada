package lab;

import java.util.*;

public class PackPrecioFijo extends Pack {

//	private double precioPack;
//	public Pack(double precio, String nombreArticulo) {
	
	public PackPrecioFijo(double precio, String nombreArticulo) {
		super(precio, nombreArticulo);
	}
	
	protected double calcularPrecio() {
		return this.precioFinal;
	}
	
	protected void agregarProducto(Producto prod) {
		this.listaProductos.add(prod);
	}
}
